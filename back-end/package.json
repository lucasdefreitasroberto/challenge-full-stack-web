{
	"name": "back-end",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"devDependencies": {
		"@babel/cli": "^7.23.4",
		"@babel/core": "^7.23.7",
		"@babel/node": "^7.22.19",
		"@babel/preset-env": "^7.23.8",
		"@babel/preset-typescript": "^7.23.3",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.11",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/node": "^20.11.5",
		"@types/supertest": "^6.0.2",
		"babel-jest": "^29.7.0",
		"babel-plugin-module-resolver": "^5.0.0",
		"dotenv-cli": "^7.3.0",
		"jest": "^29.7.0",
		"jest-mock-extended": "^3.0.5",
		"prisma": "^5.8.0",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsc": "^2.0.4",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.3.3"
	},
	"scripts": {
		"build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
		"dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
		"test": "dotenv -e .env.test -- npx prisma migrate deploy && dotenv -e .env.test -- npx prisma db seed && dotenv -e .env.test -- jest --forceExit",
		"test:unit": "dotenv -e .env.test -- jest --selectProjects unit",
		"test:e2e": "dotenv -e .env.test -- npx prisma migrate deploy && dotenv -e .env.test -- npx prisma db seed && dotenv -e .env.test -- jest --selectProjects e2e"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	},
	"dependencies": {
		"@prisma/client": "^5.8.0",
		"@types/bcrypt": "^5.0.2",
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"jsonwebtoken": "^9.0.2",
		"zod": "^3.22.4"
	}
}
